	Location = {Kyoto, Japan},
	Numpages = {9},
	Pages = {1123--1131},
	Publisher = {SIAM},
	Series = {SODA '12},
	Title = {A near-optimal sublinear-time algorithm for approximating the minimum vertex cover size},
